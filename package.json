{
  "name": "db-compat",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest",
    "test:target": "vitest run --project",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "generate": "tsx scripts/generate.ts",
    "generate:docker": "POSTGRESQL_URL=postgres://postgres:postgres@localhost:5432/test MYSQL_URL=mysql://root:mysql@localhost:3306/test tsx scripts/generate.ts",
    "generate:full": "bash scripts/generate-with-docker.sh",
    "generate:types": "tsx scripts/generate-types.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@electric-sql/pglite": "catalog:db",
    "@libsql/client": "catalog:db",
    "@planetscale/database": "catalog:db",
    "@types/better-sqlite3": "catalog:",
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "better-sqlite3": "catalog:db",
    "db0": "catalog:db",
    "eslint": "catalog:lint",
    "mysql2": "catalog:db",
    "pg": "catalog:db",
    "sqlite3": "catalog:db",
    "tsdown": "catalog:build",
    "tsx": "catalog:",
    "typescript": "catalog:types",
    "vitest": "catalog:test"
  }
}
